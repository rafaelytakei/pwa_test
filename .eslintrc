{
	"parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
		"prettier/prettier": ["error"],
		"import/no-extraneous-dependencies": "off",
		"no-restricted-syntax": "off",
		"import/no-unresolved": "off",
		"no-console": "off",
		"no-new": "off"
	},
	"env": {
		"browser": true,
		"node": true
	}
}